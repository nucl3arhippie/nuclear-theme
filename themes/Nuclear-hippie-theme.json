  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": { "foreground": "#E0C2B84D", "fontStyle": "italic" }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": { "foreground": "#3399FFE6" }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": { "foreground": "#F4F2F0E6" }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": { "foreground": "#3399FF" }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#FF005EE6", "fontStyle": "bold" }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": { "foreground": "#F4F2F0E6" }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#3399FF", "fontStyle": "bold" }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#FFDDAAE6" }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": { "foreground": "#3399FFE6" }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": { "foreground": "#FFDDAAE6" }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": { "foreground": "#FF005EE6" }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.type.class.ts", "entity.name.type.interface.ts"],
      "settings": {
        "foreground": "#3399FF"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.constant.property.ts",
        "variable.other.constant.object.property.ts"
      ],
      "settings": {
        "foreground": "#F4F2F0"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#FFDDAAE6" }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": { "foreground": "#FFDDAA80", "fontStyle": "italic" }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FF005EE6" }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": { "foreground": "#3399FFE6", "fontStyle": "bold" }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": { "foreground": "#3399FF", "fontStyle": "italic" }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": { "foreground": "#FFDDAA" }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": { "foreground": "#FFDDAAE6" }
    },
    {
      "name": "Storage keywords",
      "scope": [
        "storage",
        "keyword",
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.logical",
        "keyword.operator.expression",
        "keyword.operator.delete",
        "variable.language"
      ],
      "settings": { "foreground": "#3399FF", "fontStyle": "bold" }
    },
    {
      "name": "HTML derivative",
      "scope": "text.html.derivative",
      "settings": { "foreground": "#E0C2B8", "fontStyle": "bold" }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted",
        "string.template",
        "constant",
        "string.quoted.double",
        "string.quoted.single",
        "string.quoted.double.ts",
        "constant.numeric.decimal",
        "constant.language.null"
      ],
      "settings": { "foreground": "#A6F25A" }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": { "foreground": "#FFDDAA" }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": { "foreground": "#FFDDAA" }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": { "foreground": "#FF005E", "fontStyle": "bold" }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": { "foreground": "#3399FFE6" }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": { "foreground": "#FF005EE6" }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": { "foreground": "#FF005EE6", "fontStyle": "" }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#F4F2F0" }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": { "foreground": "#E0C2B8" }
    },
    {
      "name": "Tag brackets",
      "scope": ["punctuation.definition.tag", "punctuation.separator"],
      "settings": {
        "foreground": "#F4F2F09F"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": { "foreground": "#3399FFE6" }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": { "foreground": "#3399FFE6" }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": { "foreground": "#FFDDAA" }
    }
  ]
}
