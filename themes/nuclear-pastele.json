  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": { "foreground": "#D4C4BD4D", "fontStyle": "italic" }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": { "foreground": "#7AB8FFE6" }
    },
    { "name": "Named entities", "scope": "entity.name", "settings": { "foreground": "#E8E6E3E6" } },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": { "foreground": "#5BA3E8" }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#FF8BA7E6", "fontStyle": "bold" }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": { "foreground": "#E8E6E3E6" }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#7AB8FF", "fontStyle": "bold" }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#FFD98EE6" }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": { "foreground": "#7AB8FFE6" }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": { "foreground": "#FFD98EE6" }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": { "foreground": "#FF8BA7E6" }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.type.class.ts", "entity.name.type.interface.ts"],
      "settings": { "foreground": "#5BA3E8" }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.constant.property.ts",
        "variable.other.constant.object.property.ts"
      ],
      "settings": { "foreground": "#E8E6E3" }
    },
    { "name": "Units", "scope": "keyword.other.unit", "settings": { "foreground": "#FFD98EE6" } },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": { "foreground": "#FFD98E80", "fontStyle": "italic" }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FF8BA7E6" }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": { "foreground": "#7AB8FFE6", "fontStyle": "bold" }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": { "foreground": "#7AB8FF", "fontStyle": "italic" }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": { "foreground": "#FFD98E" }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": { "foreground": "#FFD98EE6" }
    },
    {
      "name": "Storage keywords",
      "scope": [
        "storage",
        "keyword",
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.logical",
        "keyword.operator.expression",
        "keyword.operator.delete",
        "variable.language"
      ],
      "settings": { "foreground": "#7AB8FF", "fontStyle": "bold" }
    },
    {
      "name": "HTML derivative",
      "scope": "text.html.derivative",
      "settings": { "foreground": "#D4C4BD", "fontStyle": "bold" }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted",
        "string.template",
        "constant",
        "string.quoted.double",
        "string.quoted.single",
        "string.quoted.double.ts",
        "constant.numeric.decimal",
        "constant.language.null"
      ],
      "settings": { "foreground": "#98D8AA" }
    },
    { "name": "Regex", "scope": "string.regexp", "settings": { "foreground": "#FFD98E" } },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": { "foreground": "#FFD98E" }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": { "foreground": "#FF8BA7", "fontStyle": "bold" }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": { "foreground": "#7AB8FFE6" }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": { "foreground": "#FF8BA7E6" }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": { "foreground": "#FF8BA7E6", "fontStyle": "" }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#E8E6E3" }
    },
    { "name": "Parameters", "scope": "variable.parameter", "settings": { "fontStyle": "italic" } },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": { "foreground": "#D4C4BD" }
    },
    {
      "name": "Tag brackets",
      "scope": ["punctuation.definition.tag", "punctuation.separator"],
      "settings": { "foreground": "#E8E6E39F" }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": { "foreground": "#7AB8FFE6" }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": { "foreground": "#7AB8FFE6" }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": { "foreground": "#FFD98E" }
    }
  ]
}
